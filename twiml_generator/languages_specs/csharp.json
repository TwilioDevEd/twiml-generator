{
  "language": "csharp",
  "extension": ".cs",

  "voice_class": "VoiceResponse",
  "messaging_class": "MessagingResponse",

  "text_format": "{text}, ",
  "attribute_format": "{name}: {value}",
  "use_semicolon": true,
  "append": "{parent}.{klass}({variable});",
  "chain_calls": false,


  "declare_class": true,
  "method_name_style": "camelize",
  "code_wrapper_padding": 8,
  "code_wrapper": "{imports}\n\npublic class {classname}\n{{\n    public static void Main()\n    {{\n{code}\n{print}    }}\n}}\n",

  "add_imports": "fixed",
  "import_voice": "using Twilio.TwiML;",
  "import_messaging": "using Twilio.TwiML;",
  "new_variable": "var {variable} = new {klass}({text}{attributes});",
  "new_leaf": "{parent}.{method}({text}{attributes});",
  "chained_append": ".{variable}({variable})",
  "print": "System.Console.WriteLine(response.ToString());",
  "string_quote": "\""
}